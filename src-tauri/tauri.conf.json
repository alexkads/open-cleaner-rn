{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Clean RN Dev",
  "version": "0.1.0",
  "identifier": "com.clean-rn-dev.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "Clean RN Dev",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "decorations": true,
        "transparent": false,
        "shadow": true,
        "resizable": true,
        "maximizable": true,
        "minimizable": true,
        "closable": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "sql": {
      "preload": ["sqlite:clean_rn_dev.db"]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "category": "DeveloperTool",
    "copyright": "Copyright Â© 2024 Clean RN Dev. All rights reserved.",
    "shortDescription": "Development environment cleaner for React Native projects",
    "longDescription": "Clean RN Dev is a powerful tool to clean up development environments, remove cache files, and free up disk space for React Native and other development projects.",
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "10.15",
      "exceptionDomain": "",
      "signingIdentity": null,
      "providerShortName": null,
      "entitlements": null
    },
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "linux": {
      "deb": {
        "depends": [],
        "priority": "optional",
        "section": "devel"
      },
      "appimage": {
        "bundleMediaFramework": false
      }
    }
  }
}
