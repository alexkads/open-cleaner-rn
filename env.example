# Clean RN Development Environment Variables

# Mock Configuration
# Set to '1' or 'true' to use mock services instead of real Tauri backend
# Set to '0' or 'false' to use real Tauri backend
VITE_USE_MOCK=1

# Development Settings
# Uncomment and modify as needed:

# VITE_DEBUG_MODE=1
# VITE_LOG_LEVEL=debug
# VITE_API_TIMEOUT=5000

# Database Configuration
# VITE_DB_NAME=clean_rn_dev.db

# UI Configuration
# VITE_THEME=dark
# VITE_ANIMATIONS_ENABLED=1

# Feature Flags
# VITE_ENABLE_NOTIFICATIONS=1
# VITE_ENABLE_DEBUG_PANEL=1
# VITE_ENABLE_SYSTEM_TRAY=1 